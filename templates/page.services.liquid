<div class="page-width">
  <div class="grid">
    <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
      <div class="grid">
        <div class="service-page_sidenav grid__item medium-up--one-quarter">
          <h4>CUSTOMER SERVICE</h4>
          <ul class="sidenave-service">
            {% for link in linklists.service-menu.links %}
              <li>
                <a href="{{ link.url }}">{{ link.title }}</a>
              </li>
            {% endfor %}
          </ul>
        </div>
        <div class="service-page_content grid__item medium-up--three-quarters">
          <div class="section-header">
            <h2>{{ page.title }}</h2>
          </div>
          {% if page.content.size > 0 %}
            <div class="rte">
              {{ page.content }}
            </div>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</div>
